<?xml version="1.0" encoding="utf-8" ?>
<plugin name="qaptcha">
	<title>QapTcha</title>
	<summary>Plugin adds QapTcha block to pages with submit forms</summary>
	<author>Intelliants LLC</author>
	<contributor>Intelliants LLC</contributor>
	<version>4.1.0</version>
	<date>2017-02-22</date>
	<category>captcha</category>
	<compatibility>4.1.0</compatibility>

	<phrases>
		<phrase category="common" key="qaptcha_locked">Locked : form can't be submited</phrase>
		<phrase category="common" key="qaptcha_unlocked">Unlocked : form can be submited</phrase>
	</phrases>

	<pages>
		<page name="qaptcha" url="qaptcha/" service="1">QapTcha</page>
	</pages>
</plugin>